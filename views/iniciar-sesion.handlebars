<main class="contenedor">
    <form action="/iniciar-sesion" method="POST" class="default-form">
        <div class="campo">
            <label >E-mail</label>
            <input type="email" name="email" placeholder="Tu email" >
        </div>
        <div class="campo">
            <label >Password</label>
            <input type="password" name="password" placeholder="Tu Password" >
        </div>
        <div class="campo acciones">
            <a href="/crear-cuenta">Crea una cuenta</a>
            <a href="/reestablecer-password">Olvidé mi password</a>
        </div>
        <div class="campo">
            <input type="submit" value="Iniciar Sesión" class="btn btn-vino">
        </div>
    </form>
</main>